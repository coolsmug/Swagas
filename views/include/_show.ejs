<%for(var i=0; i < items.length; i++) {%>
    <tr class="tr">
        <td class="td"><%=i + 1 %></td>
        <td class="td"><%= items[i].name %> </td>
        <td class="td"><%= items[i].job %></td>
        <td class="td"><%= items[i].state %></td>
        <td class="td"><%= items[i].email %></td>
        <td class="td"><%= items[i].number %></td>
        <td class="td"><%= items[i].username %></td>
        <td class="td">
            <a href="/update-user?id=<%= items[i]._id %>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-user-edit"></i></span>

            </a>
            <a href="#" class="btn border-shadow delete" data-id=<%= items[i]._id %>>
                <span class="text-gradient"><i class="fas fa-trash-alt"></i></span>
            </a>
        </td>
    </tr>
    <%}%> 